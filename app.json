  "window": {
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "navigationBarTitleText": "胸胸的小吕",
    "navigationStyle": "default",
    "backgroundColor": "@backgroundColor",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 10,
    "pageOrientation": "auto",
    "backgroundTextStyle": "@backgroundTextStyle"
  },
  "tabBar": {
    "color": "@color",
    "selectedColor": "@selectedColor",
    "borderStyle": "@borderStyle",
    "backgroundColor": "@tabBackgroundColor",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/component/component",
        "text": "组件",
        "iconPath": "@componentIconPath",
        "selectedIconPath": "@componentSelectedIconPath"
      },
      {
        "pagePath": "pages/expand/expand",
        "text": "扩展",
        "iconPath": "@expandIconPath",
        "selectedIconPath": "@expandSelectedIconPath"
      },
      {
        "pagePath": "pages/api/api",
        "text": "接口",
        "iconPath": "@apiIconPath",
        "selectedIconPath": "@apiSelectedIconPath"
      },
      {
        "pagePath": "pages/cloud/cloud",
        "text": "云开发",
        "iconPath": "@cloudIconPath",
        "selectedIconPath": "@cloudSelectedIconPath"
      },
      {
        "pagePath": "pages/mine/mine",
        "text": "我的",
        "iconPath": "@mineIconPath",
        "selectedIconPath": "@mineSelectedIconPath"
      }
    ]
  },
  "useExtendedLib": {
    "weui": true
  },
  "usingComponents": {
    "mp-icon": "weui-miniprogram/icon/icon",
    "mp-msg": "weui-miniprogram/msg/msg",
    "mp-cells": "weui-miniprogram/cells/cells",
    "mp-cell": "weui-miniprogram/cell/cell",
    "mp-dialog": "weui-miniprogram/dialog/dialog"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "我特么想获取你的位置"
    }
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "darkmode": true,
  "themeLocation":"theme.json",
  "lazyCodeLoading": "requiredComponents",
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "debug": false
}